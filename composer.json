{
  "name": "in2code/powermailextended",
  "description": "Example addon extension to TYPO3 extension powermail",
  "keywords": [
    "typo3",
    "form",
    "powermail",
    "powermailextended"
  ],
	"version": "1.0.0",
  "homepage": "https://forge.typo3.org/projects/extension-powermail",
  "authors": [
    {
      "name": "Alex Kellner",
      "email": "alexander.kellner@in2code.de"
    }
  ],
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "require": {
    "typo3/cms-core": "^11.5",
    "in2code/powermail": ">=9.0.0"
  },
  "autoload": {
    "psr-4": {
      "In2code\\Powermailextended\\": "Classes"
